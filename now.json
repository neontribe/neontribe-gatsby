{
  "version": 2,
  "name": "neontribe-www",
  "alias": ["neontribe.now.sh"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public",
        "useBuildUtils": "@now/build-utils@canary"
      }
    },
    {
      "src": "prototype/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/prototype",
      "status": 301,
      "headers": { "Location": "/prototype/" }
    }
  ],
  "regions": ["bru1"]
}
